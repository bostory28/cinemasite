<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="admin" namespace="/" extends="default">
		<action name="adminSearchMovieForm">
			<result>/jsp/admin/searchmovie.jsp</result>
		</action>
		<action name="adminSearchMovie" class="hkc.admin.AdminAction" method="adminSearchMovie">
			<result type="json"></result>
		</action>
		<action name="adminSelectOneMovie" class="hkc.admin.AdminAction" method="adminSelectOneMovie">
			<result type="json"></result>
		</action>
		<action name="adminInsertTimes" class="hkc.admin.AdminAction" method="adminInsertTimes">
			<result type="json"></result>
		</action>
		<action name="adminSelectTimetable" class="hkc.admin.AdminAction" method="adminSelectTimetable">
			<result type="json"></result>
		</action>
		<action name="adminTimetable">
			<result>/jsp/admin/timetable.jsp</result>
		</action>
	</package>
</struts>